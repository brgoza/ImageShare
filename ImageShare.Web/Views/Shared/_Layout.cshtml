<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ImageShare</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link href="~/css/dropzone.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://use.typekit.net/ixn8qso.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ImageShare.Web.styles.css" asp-append-version="true" />

    <script src="https://code.jquery.com/jquery-3.6.0.js"
            integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
            crossorigin="anonymous"></script>
</head>
<body>
    <vc:nav-menu></vc:nav-menu>

    <main role="main">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </main>

    <div class="footer text-muted">
        &copy; 2022 - ImageShare.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/dropzone@6.0.0-beta.1/dist/dropzone-min.js"></script>
    @RenderSection("Scripts",false)
</body>
</html>
